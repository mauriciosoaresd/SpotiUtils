{
    "version": 2,
    "builds": [
        {
            "src": "/",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "server.js"
        },
        {
            "src": "/",
            "dest": "build/index.html"
        }
    ]
}