{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server.js"
        }
    ]
  }
  {
    "version": 2,
    "builds": [
      { "src": "build", "use": "@vercel/static"},
      { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/server.js"
      }
    ]
  }